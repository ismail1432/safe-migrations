{
  "type": "project",
  "name": "eniams/safe-migrations",
  "description": "Warn you when a migration is unsafe",
  "license": "proprietary",
  "minimum-stability": "stable",
  "prefer-stable": true,
  "require": {
    "php": "^8.1.0",
    "symfony/console": "^5.4",
    "symfony/http-kernel": "^5.4",
    "symfony/config": "^5.4",
    "symfony/dependency-injection": "^5.4",
    "symfony/finder": "^5.4"
  },
  "require-dev": {
    "phpstan/phpstan": "^1.10",
    "phpunit/phpunit": "^9.5",
    "symfony/framework-bundle": "^5.4",
    "symfony/browser-kit": "^5.4",
    "symfony/yaml": "^5.4"
  },
  "autoload": {
    "psr-4": {
      "Eniams\\SafeMigrationsBundle\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Eniams\\SafeMigrationsBundle\\Tests\\": "tests/"
    }
  }
}
